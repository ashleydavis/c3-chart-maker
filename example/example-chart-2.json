{
    "size": {
        "height": 600,
        "width": 1200
    },        
    "data": {
        "keys": {
            "x": "Date",
            "value": ["Close", "Volume"]
        },
        "type": "line",
        "axes": {
            "Volume": "y2"
        }
    },
    "axis": {
        "x": {
            "type": "timeseries",
            "tick": {
                "format": "%Y-%m-%d"
            }
        },
        "y": {
            "label": {
                "text": "Close",
                "position": "outer-middle"
            }
        },
        "y2": {
            "show": true,
            "label": {
                "text": "Signal",
                "position": "outer-middle"
            }
        }
    },
    "transition": {
        "duration": 0
    }
}